{
  "name": "auto-cookie",
  "version": "2.0.0",
  "description": "auto set cookie domain like google-analytics",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/harikitech/auto-cookie.git"
  },
  "author": "harikitech",
  "homepage": "https://github.com/harikitech/auto-cookie#readme",
  "bugs": {
    "url": "https://github.com/harikitech/auto-cookie/issues"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc && webpack --env=production",
    "clean": "rimraf lib",
    "lint": "run-s -l lint:*",
    "lint:eslint": "eslint . --ext=ts,tsx",
    "lint:prettier": "prettier {src,test}/**/*.ts --check",
    "lint:ts": "tsc -p . --noEmit",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "prestart": "webpack",
    "pretest": "webpack",
    "start": "ts-node server.ts",
    "test": "wdio wdio.conf.js"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "cookie"
  ],
  "peerDependencies": {
    "js-cookie": "2.x"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-typescript": "^7.0.3",
    "@types/finalhandler": "1.1.0",
    "@types/js-cookie": "2.2.6",
    "@types/mocha": "^8.0.0",
    "@types/power-assert": "1.5.3",
    "@types/serve-static": "1.13.4",
    "@wdio/cli": "^6.0.9",
    "@wdio/local-runner": "^6.0.9",
    "@wdio/mocha-framework": "^6.0.8",
    "@wdio/spec-reporter": "^6.0.8",
    "@wdio/sync": "^6.0.8",
    "chromedriver": "^87.0.1",
    "eslint": "^7.0.0",
    "eslint-plugin-mocha": "^8.0.0",
    "espower-typescript": "9.0.2",
    "finalhandler": "1.1.2",
    "husky": "^4.2.3",
    "js-cookie": "2.2.1",
    "lint-staged": "^10.1.1",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1",
    "prettier": "^2.0.2",
    "prettier-plugin-package": "1.3.0",
    "rimraf": "3.0.2",
    "serve-static": "1.14.1",
    "size-plugin": "^2.0.1",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "wdio-chromedriver-service": "^6.0.1",
    "webpack": "5.11.1",
    "webpack-cli": "4.3.1"
  },
  "types": "lib/index.d.ts",
  "eslintConfig": {
    "extends": [
      "@9renpoto/eslint-config-typescript"
    ],
    "plugins": [
      "mocha"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
