    "eslint": "^4.17.0",
    "espower-typescript": "^8.1.3",
    "finalhandler": "^1.1.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "js-cookie": "^2.2.0",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.1",
    "power-assert": "^1.4.4",
